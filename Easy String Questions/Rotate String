// rotate string

#include <iostream>
#include <string>
using namespace std;

bool rotateString(string s, string goal) {
    return s.length() == goal.length() && (s+s).find(goal) != -1;
}

int main() {
    string s = "abcde";
    string goal = "cdeab";
    cout << rotateString(s, goal) << endl;// true

    s = "abcde";
    goal = "abced";
    cout << rotateString(s, goal) << endl;// false

    return 0;
}
